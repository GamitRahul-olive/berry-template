<footer class="footer-section footer4" style="{% if section.settings.bg_image %}background-image: url({{ section.settings.bg_image | img_url: 'master'}});{% else %}background-color: {{ section.settings.bg_color }};{% endif %}">  
  <div class="{% if section.settings.footer_width == 'standard-width' %}page-width{% else %}container-fluid{% endif %}" style="padding: {{ section.settings.section_padding }};">
  {% if section.settings.top_footer_show %}
    <section class="footer_top {% if section.settings.footer_width == 'full-width' %}page-width{% endif %}">
      <div class="row top_footer">
        <div class="col-md-6">
         {% if section.settings.top_footer_log %}
          <div class="footer_logo">
            <a href="{{ routes.root_url }}"><img src="{{ section.settings.top_footer_log | img_url:'master' }}" alt=""></a>
          </div>
          {% endif %}
          {{ section.settings.top_footer_text }}
        </div>
        {% if section.settings.social_media_icon_show %} 
          <div class="col-md-6 d-flex justify-content-xl-end justify-content-start">
            <div class="top_footer_sec social-section">
               {% if section.settings.social_media_title %}<h4 style="color: {{ section.settings.social-media-title-color }};">{{ section.settings.social_media_title }}</h4> {% endif %}
                <ul class="footer-menu-dev">
                  {% if section.settings.social_facebook_link != blank %}
                  <li class="facebook">
                    <a href="{{ section.settings.social_facebook_link | escape }}" aria-label="facebook"><i class="fa fa-facebook" aria-hidden="true"></i>Facebook</a>
                  </li>
                  {% endif %}
                  {% if section.settings.social_google_link != blank %}
                  <li class="google">
                    <a href="{{ section.settings.social_google_link | escape }}" aria-label="google-plus"><i class="fa fa-google-plus" aria-hidden="true"></i>Google</a>
                  </li>
                  {% endif %}
                  {% if section.settings.social_twitter_link != blank %}
                  <li class="twitter">
                    <a href="{{ section.settings.social_twitter_link | escape }}" aria-label="twitter"><i class="fa fa-twitter" aria-hidden="true"></i>Twitter</a>
                  </li>
                  {% endif %}
                  {% if section.settings.social_instagram_link != blank %}
                  <li class="insta">
                    <a href="{{ section.settings.social_instagram_link | escape }}" aria-label="instagram"><i class="fa fa-instagram" aria-hidden="true"></i>Instagram</a>
                  </li>
                  {% endif %}
                  {% if section.settings.social_pinterest_link != blank %}
                  <li class="youtube">
                    <a href="{{ section.settings.social_pinterest_link | escape }}" aria-label="pinterest"><i class="fa fa-pinterest" aria-hidden="true"></i>Youtube</a>
                  </li>
                  {% endif %}
                  {% if section.settings.social_rss_link != blank %}
                  <li class="rss">
                    <a href="{{ section.settings.social_rss_link | escape }}" aria-label="rss"><i class="fa fa-rss" aria-hidden="true"></i></a>
                  </li>
                  {% endif %}
                  {% if section.settings.social_youtube_link != blank %}
                  <li class="youtube">
                    <a href="{{ section.settings.social_youtube_link | escape }}" aria-label="youtube"><i class="fa fa-youtube-play" aria-hidden="true"></i>Youtube</a>
                  </li>
                  {% endif %}
                </ul>
              </div>
          </div>
        {% endif %}
      </div>
    </section>
  {% endif %}  
    <section class="footer-outer {% if section.settings.footer_width == 'full-width' %}page-width{% endif %}">
      <div class="row footer-theme">
        {% for block in section.blocks %}
        {% capture blockId %}{{ block.id }}{% endcapture %}
        {% if block.type == 'store-content' %}
          <div class="col address-block">
            <div class="footer-title footer-desktop-title">
              <h4 style="color: {{ block.settings.contact_info_title_color }}" >{{ block.settings.contact_info_title }}</h4>
            </div>            
            <ul>
            {% for j in (1..5) %}              
              {% capture contact_info_icon %}icon_img_{{ j }}{% endcapture %}
              {% capture contact_info_text %}contact_info_text_{{ j }}{% endcapture %}
              <li>
                {% if block.settings[contact_info_icon] !=blank %}
                  <div class="contact_info_icon"><img src="{{ block.settings[contact_info_icon] | img_url:'master' }}" alt=""></div>
                {% endif %}  
                {% if block.settings[contact_info_text] !=blank %}
                  <div class="contact_info_text">{{ block.settings[contact_info_text] }}</div>
                {% endif %}  
              </li>
            {% endfor %}
            </ul>
          </div>
        {% elsif block.type == 'newsletter' %}
          <div class="col">
            <div class="newsletter-section">
              <div class="subscribe-white">
                <div class="footer-contant">
                  <div class="news-inner">
                    <div class="footer-title footer-desktop-title">
                      <h4>
                        {{ block.settings.ntitle }}
                      </h4>
                    </div>
                    <div class="ndesc">
                      {% if block.settings.ndesc %}
                        <p>
                          {{ block.settings.ndesc }}
                        </p>
                      {% endif %}
                    </div>
                    {%- form 'customer', id: 'ContactFooter', class: 'footer__newsletter' -%}
                      <div id="mc_embed_signup_scroll">
                        <div class="form-group">
                          
                          <input type="email" class="required email form-control" name="contact[email]" value="{{ form.email }}" aria-required="true" autocorrect="off"
                                 autocapitalize="off" autocomplete="email" {% if form.errors %} autofocus aria-invalid="true" aria-describedby="ContactFooter-error" {% elsif form.posted_successfully? %} aria-describedby="ContactFooter-success" {% endif %} placeholder="{{ 'general.newsletter_form.email_placeholder' | t }}" required>                        
                          <button type="submit" class="btn btn-prinmary" name="subscribe" id="mc-embedded-subscribe">
                            <span class="mobile-up">{{ 'general.newsletter_form.submit' | t }}</span>
                            <span class="mobile-down">{{ 'general.newsletter_form.submit2' | t }}</span>
                          </button>
                        </div>
                        <div id="mce-responses" class="clear">
                          <div class="response" id="mce-error-response" style="display:none"></div>
                          <div class="response" id="mce-success-response" style="display:none"></div>
                        </div>
                        <!-- real people should not fill this in and expect good things - do not remove this or risk form bot signups-->
                        <div style="position: absolute; left: -5000px;" hidden>
                          <input type="text" name="b_17af379706d80b694776f991f_9ebb72e4d2" tabindex="-1" value="">
                        </div>                        
                        {%- if form.errors -%}
                          <small class="newsletter-form__message form__message" id="ContactFooter-error">{% render 'icon-error' %}{{ form.errors.translated_fields['email'] | capitalize }} {{ form.errors.messages['email'] }}</small>
                        {%- endif -%}
                      </div>
                      {%- if form.posted_successfully? -%}
                        <h3 class="newsletter-form__message newsletter-form__message--success form__message" id="ContactFooter-success" tabindex="-1" autofocus>{% render 'icon-success' %}{{ 'newsletter.success' | t }}</h3>
                      {%- endif -%}
                    {%- endform -%}
                  </div>
                </div>
              </div>
            </div>
          </div>
        {% elsif block.type == 'linklist' %}          
          <div class="col footer-link">
            <div class="footer-title footer-desktop-title">
              <h4 style="color: {{ block.settings.footer-title-color }};">
                {{ block.settings.ltitle }}
              </h4>
            </div>
            <ul class="dynamic-footer-menu">
              {% if block.settings.footer_linklist != blank %} 
              {% assign footer_linklist = block.settings.footer_linklist -%}
              {% for link in linklists[footer_linklist].links %}
              <li>
                {% assign data_title = link.title | split: '|' | first %}
                {% assign data_json = link.title | split: '|' | last %}
                <a href="{{ link.url }}" style="color:{{ block.settings.footer-menu-color }};">
                  {{ data_title }}
                </a>
              </li>
              {% endfor %}
              {% endif %}
            </ul>
          </div>
        {% endif %}
